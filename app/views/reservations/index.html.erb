<h1>Reservations#index</h1>
<p>Find me in app/views/reservations/index.html.erb</p>
<h1>予約済み一覧</h1>
<table>
  <thead>
    <tr>
     <th>ルームid</th>
      <th>ルーム画像</th>
      <th>ルーム名</th>
      <th>ルーム紹介</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>予約確定日時</th>
    </tr>
  </thead>
  <tbody>
　<% @reservations.each do |reservation| %>
    <tr>
      <td><%= reservation.room_id %></td>
      <td><%= link_to image_tag reservation.room.room_image.to_s%></td>
       <td><%= reservation.room.room_name%></td>
         <td><%= reservation.room.room_introduction%></td>
<td><%= reservation.start_date.to_s(:datetime_jp) %></td>
<td><%= reservation.end_date.to_s(:datetime_jp) %></td>
<td><%= reservation.created_at.to_s(:datetime_jp) %></td>
</tr>
  <% end %>
  </tbody>
</table>






