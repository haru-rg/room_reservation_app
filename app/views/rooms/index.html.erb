<h1>Rooms#index</h1>
<p>検索結果：</p>

<table>
    <thead>
        <tr>
            <th>room_id</th>
            <th>room_image</th>
            <th>room_name</th>
            <th>room_address</th>
            <th>room_price</th>
        </tr>
    </thead>

    <tbody>
        <% @rooms.each do |room| %>
            <tr>
                <td>
                    <%= room.id %>
                </td>
                <td>
                    <%= room.room_image %>
                </td>
                <td>
                    <%= room.room_name %>
                </td>
                <td>
                    <%= room.room_address %>
                </td>
                <td>
                    <%= room.room_price %>
                </td>
            </tr>
            <% end %>
    </tbody>
</table>